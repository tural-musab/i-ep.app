{
  "name": "i-ep.app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:test": "NODE_ENV=test next dev",
    "dev:staging": "NODE_ENV=staging next dev",
    "dev:local-remote": "NODE_ENV=development NEXT_ENV_PROFILE=local-remote next dev",
    "build": "next build",
    "build:staging": "NODE_ENV=staging next build",
    "build:local-remote": "NODE_ENV=development NEXT_ENV_PROFILE=local-remote next build",
    "start": "next start",
    "start:staging": "NODE_ENV=staging next start",
    "start:local-remote": "NODE_ENV=development NEXT_ENV_PROFILE=local-remote next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "jest --coverage && open coverage/lcov-report/index.html",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration --runInBand --verbose",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "validate:env": "tsx src/env.ts",
    "create-tenant": "node scripts/create-tenant.js",
    "create-tenant-config": "node scripts/create-tenant.js",
    "docs:dev": "cd docs-site && npm run start",
    "docs:build": "cd docs-site && npm run build",
    "docs:check-links": "cd docs-site && npm run build 2>&1 | grep -i 'broken' && exit 1 || echo 'No broken links found'",
    "docs:spellcheck": "cspell 'docs-site/docs/**/*.md'",
    "test:rls": "jest --testPathPattern=rls-bypass --verbose",
    "test:security": "jest --testPathPattern=security --verbose",
    "zap:scan": "docker run --network host -v ./zap-reports:/zap/wrk ghcr.io/zaproxy/zaproxy:stable zap-full-scan.py -t http://localhost:3000 -j -I -r zap.html -w zap.sarif",
    "test:security:full": "npm run zap:scan && jest --testPathPattern=security"
  },
  "dependencies": {
    "@auth/supabase-adapter": "^1.8.0",
    "@hookform/resolvers": "^4.1.3",
    "@notionhq/client": "^4.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/nextjs": "^9.35.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/postgrest-js": "^1.19.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.1",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-table": "^8.21.2",
    "@types/express-rate-limit": "^5.1.3",
    "@upstash/redis": "^1.34.5",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "cloudflare": "^4.1.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "express-rate-limit": "^7.5.1",
    "gray-matter": "^4.0.3",
    "iyzipay": "^2.0.64",
    "lucide-react": "^0.294.0",
    "memory-store": "^0.0.1",
    "next": "15.2.2",
    "next-auth": "^4.24.11",
    "next-mdx-remote": "^5.0.0",
    "next-swagger-doc": "^0.4.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^3.0.1",
